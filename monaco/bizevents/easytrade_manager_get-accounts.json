{
  "enabled": true,
  "ruleName": "{{ .name }}",
  "triggers": [
    {
      "source": {
        "dataSource": "request.method"
      },
      "type": "EQUALS",
      "value": "GET",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "EQUALS",
      "value": "/api/Accounts",
      "caseSensitive": false
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytrade.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytrade.manager.accounts"
    },
    "category": {
      "sourceType": "request.path"
    },
    "data": []
  }
}
