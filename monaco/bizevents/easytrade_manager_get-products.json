{
  "enabled": true,
  "ruleName": "EasyTrade [manager | .NET] - GET /api/Products/GetProducts",
  "triggers": [
    {
      "source": {
        "dataSource": "request.method"
      },
      "type": "EQUALS",
      "value": "GET",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "EQUALS",
      "value": "/api/Products/GetProducts",
      "caseSensitive": false
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytrade.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytrade.manager.products"
    },
    "category": {
      "sourceType": "request.path"
    },
    "data": [
      {
        "name": "array",
        "source": {
          "sourceType": "constant.string",
          "source": "true"
        }
      },
      {
        "name": "firstId",
        "source": {
          "sourceType": "response.body",
          "path": "0.id"
        }
      },
      {
        "name": "firstName",
        "source": {
          "sourceType": "response.body",
          "path": "0.name"
        }
      },
      {
        "name": "firstPrice",
        "source": {
          "sourceType": "response.body",
          "path": "0.price"
        }
      },
      {
        "name": "firstSupport",
        "source": {
          "sourceType": "response.body",
          "path": "0.support"
        }
      },
      {
        "name": "fullArray",
        "source": {
          "sourceType": "response.body",
          "path": "*"
        }
      }
    ]
  }
}
