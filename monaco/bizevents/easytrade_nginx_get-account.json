{
  "enabled": true,
  "ruleName": "{{ .name }}",
  "triggers": [
    {
      "source": {
        "dataSource": "request.method"
      },
      "type": "EQUALS",
      "value": "GET",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "N_CONTAINS",
      "value": "presets",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "STARTS_WITH",
      "value": "/accountservice/api/accounts",
      "caseSensitive": true
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytrade.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytrade.nginx.account"
    },
    "category": {
      "sourceType": "request.path"
    },
    "data": [
      {
        "name": "username",
        "source": {
          "sourceType": "response.body",
          "path": "username"
        }
      },
      {
        "name": "preset",
        "source": {
          "sourceType": "response.body",
          "path": "preset"
        }
      },
      {
        "name": "packageId",
        "source": {
          "sourceType": "response.body",
          "path": "packageId"
        }
      },
      {
        "name": "packageActivationDate",
        "source": {
          "sourceType": "response.body",
          "path": "packageActivationDate"
        }
      },
      {
        "name": "origin",
        "source": {
          "sourceType": "response.body",
          "path": "origin"
        }
      },
      {
        "name": "lastName",
        "source": {
          "sourceType": "response.body",
          "path": "lastName"
        }
      },
      {
        "name": "accountId",
        "source": {
          "sourceType": "response.body",
          "path": "id"
        }
      },
      {
        "name": "hashedPassword",
        "source": {
          "sourceType": "response.body",
          "path": "hashedPassword"
        }
      },
      {
        "name": "firstName",
        "source": {
          "sourceType": "response.body",
          "path": "firstName"
        }
      },
      {
        "name": "email",
        "source": {
          "sourceType": "response.body",
          "path": "email"
        }
      },
      {
        "name": "creationDate",
        "source": {
          "sourceType": "response.body",
          "path": "creationDate"
        }
      },
      {
        "name": "address",
        "source": {
          "sourceType": "response.body",
          "path": "address"
        }
      },
      {
        "name": "accountActive",
        "source": {
          "sourceType": "response.body",
          "path": "accountActive"
        }
      }
    ]
  }
}
