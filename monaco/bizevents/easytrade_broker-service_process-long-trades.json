{
  "enabled": true,
  "ruleName": "{{ .name }}",
  "triggers": [
    {
      "source": {
        "dataSource": "request.method"
      },
      "type": "EQUALS",
      "value": "POST",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "STARTS_WITH",
      "value": "/v1/trade/long/process",
      "caseSensitive": false
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytrade.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytrade.process-long-trades"
    },
    "category": {
      "sourceType": "request.path"
    },
    "data": []
  }
}
