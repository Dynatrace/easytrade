{
  "enabled": true,
  "ruleName": "{{ .name }}",
  "triggers": [
    {
      "source": {
        "dataSource": "request.method"
      },
      "type": "EQUALS",
      "value": "GET",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "N_CONTAINS",
      "value": "long",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "N_CONTAINS",
      "value": "sell",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "N_CONTAINS",
      "value": "buy",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "STARTS_WITH",
      "value": "/broker-service/v1/trade",
      "caseSensitive": false
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytrade.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytrade.nginx.trades"
    },
    "category": {
      "sourceType": "request.path"
    },
    "data": [
      {
        "name": "onlyLong",
        "source": {
          "sourceType": "request.parameters",
          "path": "onlyLong"
        }
      },
      {
        "name": "onlyOpen",
        "source": {
          "sourceType": "request.parameters",
          "path": "onlyOpen"
        }
      },
      {
        "name": "page",
        "source": {
          "sourceType": "request.parameters",
          "path": "page"
        }
      },
      {
        "name": "count",
        "source": {
          "sourceType": "request.parameters",
          "path": "count"
        }
      }
    ]
  }
}
