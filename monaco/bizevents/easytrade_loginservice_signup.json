{
  "enabled": true,
  "ruleName": "{{ .name }}",
  "triggers": [
    {
      "source": {
        "dataSource": "request.method"
      },
      "type": "EQUALS",
      "value": "POST",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "EQUALS",
      "value": "/api/Signup",
      "caseSensitive": false
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytrade.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytrade.signup"
    },
    "category": {
      "sourceType": "request.path"
    },
    "data": [
      {
        "name": "accountId",
        "source": {
          "sourceType": "response.body",
          "path": "id"
        }
      },
      {
        "name": "address",
        "source": {
          "sourceType": "request.body",
          "path": "address"
        }
      },
      {
        "name": "origin",
        "source": {
          "sourceType": "request.body",
          "path": "origin"
        }
      },
      {
        "name": "email",
        "source": {
          "sourceType": "request.body",
          "path": "email"
        }
      },
      {
        "name": "userName",
        "source": {
          "sourceType": "request.body",
          "path": "username"
        }
      },
      {
        "name": "lastName",
        "source": {
          "sourceType": "request.body",
          "path": "lastName"
        }
      },
      {
        "name": "firstName",
        "source": {
          "sourceType": "request.body",
          "path": "firstName"
        }
      },
      {
        "name": "packageId",
        "source": {
          "sourceType": "request.body",
          "path": "packageId"
        }
      }
    ]
  }
}
