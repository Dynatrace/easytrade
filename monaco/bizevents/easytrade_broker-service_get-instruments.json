{
  "enabled": true,
  "ruleName": "EasyTrade [broker-service | .NET] - GET /v1/instrument",
  "triggers": [
    {
      "source": {
        "dataSource": "request.method"
      },
      "type": "EQUALS",
      "value": "GET",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "STARTS_WITH",
      "value": "/v1/instrument",
      "caseSensitive": false
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytrade.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytrade.instruments"
    },
    "category": {
      "sourceType": "request.path"
    },
    "data": [
      {
        "name": "array",
        "source": {
          "sourceType": "constant.string",
          "source": "true"
        }
      },
      {
        "name": "accountId",
        "source": {
          "sourceType": "request.parameters",
          "path": "accountId"
        }
      },
      {
        "name": "firstId",
        "source": {
          "sourceType": "response.body",
          "path": "results.0.id"
        }
      },
      {
        "name": "firstCode",
        "source": {
          "sourceType": "response.body",
          "path": "results.0.code"
        }
      },
      {
        "name": "firstName",
        "source": {
          "sourceType": "response.body",
          "path": "results.0.name"
        }
      },
      {
        "name": "firstProductId",
        "source": {
          "sourceType": "response.body",
          "path": "results.0.productId"
        }
      },
      {
        "name": "firstAmount",
        "source": {
          "sourceType": "response.body",
          "path": "results.0.amount"
        }
      },
      {
        "name": "firstPriceClose",
        "source": {
          "sourceType": "response.body",
          "path": "results.0.price.close"
        }
      }
    ]
  }
}
